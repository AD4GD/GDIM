        skos:inScheme               base:eav ;
        skos:prefLabel              "Leaf Area Index" ;
        dcat:keyword                "Productivity" ;
        base:agriLandIncludes       "Agriculture Mask" ;
        base:categoryGEOGLAM        "Productivity" ;
        base:policySupported        "Carbon Modelling Yield" ;
        base:updateFrequency        "Twice weekly or better" ;
        base:spatialResolutionUnit  "Field mapping at 10m" ;
        base:stewards               "Andrew M. Davidson, Heather McNairn" .

<http://w3id.org/ad4gd/ev/eav/IrrigatedCroplandMap>
        rdf:type                    skos:Concept ;
        rdf:type                    base:EAV ;
        dct:identifier              7 ;
        rdfs:isDefinedBy            <https://www.agvariables.org/full-eav-table> ;
        skos:definition             "A binary determination of where managed water was applied to aid in the growth of perennial and non-perennial crops as well as managed grasslands." ;
        skos:inScheme               base:eav ;
        skos:prefLabel              "Irrigated Cropland Map" ;
        dcat:keyword                "Land Use" ;
        base:agriLandIncludes       "Perennial and Non-Perennial Croplands, managed grasslands" ;
        base:categoryGEOGLAM        "Land Use" ;
        base:policySupported        "SDG 6.4, drought, climate adaptation, irrigation management" ;
        base:updateFrequency        "Generated per cropping cycle for non-perennial crops, or per year in the case of perennial crop types" ;
        base:spatialResolutionUnit  "Field to county or equivalent admin scale (as percentage)" ;
        base:stewards               "Jippe Hoogeveen" .

<http://w3id.org/ad4gd/ev/eav/DegreeGrowingDays>
        rdf:type                    skos:Concept ;
        rdf:type                    base:EAV ;
        dct:identifier              8 ;
        rdfs:isDefinedBy            <https://www.agvariables.org/full-eav-table> ;
        skos:definition             "Growing degree days for a given crop are calculated each day, as the average of the daily maximum and minimum temperatures, minus a certain base temperature, where this base temperature represents the threshold under which that crop will not grow. \"Growing degree days\" are a heuristic tool that measures heat accumulation for crops from sowing through harvest, with phenological transitions occurring after a certain accumulation of growing degree days." ;
        skos:inScheme               base:eav ;
        skos:prefLabel              "Degree Growing Days" ;
        dcat:keyword                "Productivity" ;
        base:agriLandIncludes       "Non-Perennial Cropland Mask, with priority given to crops (as determined by Non-Perennial Crop Type Map) for whom GDD are a known correlate with phenological development." ;
        base:categoryGEOGLAM        "Productivity" ;
        base:policySupported        "Tracking phenological development, linking to the drought" ;
        base:updateFrequency        "Daily" ;
        base:spatialResolutionUnit  "0.10 degree or finer" ;
        base:stewards               "Belen Franch, Jinlong Fan" .

<http://w3id.org/ad4gd/ev/eav/FractionalCover>
        rdf:type               skos:Concept ;
        rdf:type               base:EAV ;
        dct:identifier         9 ;
        rdfs:isDefinedBy       <https://www.agvariables.org/full-eav-table> ;
        skos:definition        "Fractions (of an area or the pixel) of photosynthetic (green) vegetation and non-photosynthetic (dry, litter) vegetation and the remaining fraction of bare soil, including stones and rock." ;
        skos:inScheme          base:eav ;
        skos:prefLabel         "Fractional Cover" ;
        dcat:keyword           "Land Use, Productivity" ;
        base:agriLandIncludes  "Agriculture Mask" ;
        base:categoryGEOGLAM   "Land Use, Productivity" ;
        base:policySupported   "Early warning for food security; Changes in the agricultural landscape; Land degradation" ;
        base:updateFrequency   "20m - 500m" ;
        base:stewards          "Juan Pablo Guerschman" .

