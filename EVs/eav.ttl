<http://w3id.org/ad4gd/ev/eav/CropResidueCoverPercentage>
        rdf:type                    skos:Concept ;
        rdf:type                    base:EAV ;
        dct:identifier              14 ;
        rdfs:isDefinedBy            <https://www.agvariables.org/full-eav-table> ;
